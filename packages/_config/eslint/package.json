{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@topp/_config.eslint",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/lib.ts",
      "default": "./target/tsc/src/lib.js"
    },
    "./*": {
      "types": "./src/*.ts",
      "default": "./target/tsc/src/*.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "check::prettier": "prettier --ignore-path=../../../.gitignore --check ."
  },
  "dependencies": {
    "@eslint/js": ">=8 <9",
    "@typescript-eslint/eslint-plugin": ">=6 <7",
    "@typescript-eslint/parser": ">=6 <7"
  },
  "peerDependencies": {
    "@eslint/eslintrc": ">=2 <3",
    "eslint": ">=8 <9",
    "eslint-config-next": ">=13 <14",
    "typescript": ">=5 <6"
  },
  "peerDependenciesMeta": {
    "@eslint/eslintrc": {
      "optional": true
    },
    "eslint-config-next": {
      "optional": true
    }
  }
}
