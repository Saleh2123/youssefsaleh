{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@topp/_config.next",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/lib.ts",
      "default": "./target/tsc/src/lib.js"
    },
    "./*": {
      "types": "./src/*.ts",
      "default": "./target/tsc/src/*.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "check::eslint": "TIMING=1 eslint .",
    "check::prettier": "prettier --ignore-path=../../../.gitignore --check ."
  },
  "peerDependencies": {
    "next": ">=13 <14"
  }
}
